{
  "compilerOptions": {
    "target": "ES2020",// cho phép biên dịch code typescript sang kiểu es6
    "module": "CommonJS",// commónjs là tiêu chuẩn được sử dụng trong nodejs để làm vc vs các module
    "moduleResolution": "node", // xóa comment khi biên dịch sang file js
    "outDir": "dist",
    "esModuleInterop": true, // dùng từ khóa import thay cho require
  }
}
